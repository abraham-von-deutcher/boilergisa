body{
background-color:#cccccc;
}
.kindsInstall{
width: 100%;
display: flex;
padding-top:56px;
padding-left: 8px;
padding-right: 8px;
margin: 0;
background-color: #ffffff;
}
.findInner{
width:100%;
padding:10px 16px;
}
#setting-region{
width:100%;	
border: 1px solid #bebebe;
border-radius: 10px;
}
#setting-region p{
margin:10px;
font-weight: bolder;
color: #929292;
}
.resultRegion{
font-size: 13px;
margin-left:8px;
font-weight: bolder;
}
#setting-region img{
width: 23px;
height: 28px;
margin: 6px;
padding: 0;
display: flex;
float: left;
}
.orderContent{
height:100%;
padding-bottom:63px;
font-size: 12px;
}
.orderList li{
height: 336px;
border: 1px solid #d1d1d1;
background: #ffffff;
margin: 10px;
padding:8px;
flex-direction: column;
}
.orderList li:hover{
background:#efefef;
cursor:pointer;
}
.engnrInfo{
width: 100%;
height: 35px;
display: flex;
font-size: 13px;
border-bottom: 0.5px solid #909090;
}
.engnrInfo img{
width: 15%;
margin: 4px;
}
.engnrInfo .engnrName{
width: 55%;
text-align: center;
align-self: center;
font-size: 11px;
white-space: nowrap;
overflow: hidden;
text-overflow: ellipsis;
}
.engnrInfo .recordDate{
width: 30%;
align-self: center;
text-align: right;
padding: 0 8px;
}
.installInfo{
width: 100%;
text-align: center;
}
.installInfo .installEqu{
font-size: 14px;
font-weight: bold;
padding: 4px;
margin: 0;
}
.installInfo .installAddr{
padding: 4px;
margin: 0;
}
.etcList p{
font-size: 13px;
text-align: center;
padding:10px;
}
.recordImg{
width: 100%;
height: 196px;
}
.recordImg img{
width: 100%;
height: 100%;
}
.recordCommentary{
width:100%;
height:36px;
padding-top: 8px;
}
.recordHit{
float:right;
padding: 5.5px;
}
.addRecord{
width: 60px;
height: 40px;;
color: #ffffff;
border: none;
border-radius: 25%;
font-size: 13px;
padding: 10px;
right: 30px;
position:fixed;
bottom:90px;
background:#FF9900;
}
.recordCommentary .totalRating{
color:#FF0000;
font-weight:bold;
}