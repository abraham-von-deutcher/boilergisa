.main{
padding-top: 56px;
}
.main ul{
clear: left;
width: 100%;
padding: 0;
margin: 0;
position: relative;
list-style: none;
line-height: 5vh;
background-color: #fff;
}
.main ul li{
border-bottom: 1px solid #d2d4d4;

}
.main ul li:hover{
background: #eeeeee;
}
.main ul li ul{
width: 100%;
border-top: 1px solid #d2d4d4;
}         
.main ul li ul li{    
font-size: 12px;
padding: 0;
margin: 0;
border-right: 1px solid #d2d4d4;
text-align: center;
width: 33%;
display: inline-block;
}
.main summary{
padding-left: 25px;
}

details[open] summary::-webkit-details-marker {
color: transparent;
width: 20px;
height: 40px;
position: absolute;
left: 0;
padding: 0;
margin: 0;
}
details summary::-webkit-details-marker{
color: transparent;
width: 20px;
height: 40px;
position: absolute;
left: 0;
padding: 0;
margin: 0;
}
.explain-region{
font-size: 12px;
margin-bottom: 120px;
text-align: center;
}
#result-region{
width: 100%;
height: 110px;
background-color: rgba(226, 226, 226, 0.658);
bottom: 0;
position :fixed;
z-index:1;
}
#result-region p{
padding: 10px;
}
#select-region{
width: 100%;
height: 60px;
bottom: 0;
padding: 0;
margin: 0;
position :fixed;
z-index:1;
background-color: rgba(54, 185, 255, 0.83);

}
#select-region span{
color: #fff;
}