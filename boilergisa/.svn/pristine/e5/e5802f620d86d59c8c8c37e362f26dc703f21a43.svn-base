.detailQuote{
padding:16px 24px 30px 16px;	
}
.quoteInfoBox{
width:100%;
display: flex;
padding-bottom: 16px;
align-items: center;
align-self: center;
align-content: center;
text-align: center;
}
.quoteText{
width: 20%;
font-size:13px;
text-align: center;
align-items: center;
align-self: center;
align-content: center;
}
.quoteInfoBox input{
width:55%;
padding: 0 10px;
font-size: 20px;
text-align: right;
font-weight: bold;
color: #FF9800;
}
.quoteInfoBox .quoteWon{
width:25%;
font-size:12px;
text-align: center;
align-items: center;
align-self: center;
align-content: center;
}
.quoteInfoBox textarea{
width:80%;
font-size:12px;
}
.quoteResponse{
width: 100%;
position: fixed;
color:#ffffff;
bottom: 0;
right: 0;
left: 0;
padding: 0;
margin: 0;
}
.quoteBtn{
width: 100%;
height: 50px;
background-color: #ff9900dc;
border: none;
outline: none;
font-size: 1em;
}
.quoteInfo{
display:flex;
flex-direction: column;
align-items: center;
text-align: center;
padding-bottom:16px;
}
.installCondition{
width:100%;
display:flex;
flex-direction: row;
padding-bottom: 16px;
text-align:center;
}
.installCondition .installEquipment{
width: 30%;
}
.InfoInstalled{
width: 100%;
display: flex;
flex-direction: row;
text-align:center;
}
.InfoInstalled .installedFuel{
width: 33%;
padding-left: 2%;
}
.InfoInstalled .installedMethod{
width: 33%;
padding: 0 2%;
}
.InfoInstalled .installedVolume{
width: 34%;
}
.acceptQuote .acceptBtn{
width: 100%;
height: 40px;
padding: 0px;
margin: 5px;
border: none;
outline: none;
background: #FF9800;
color:#ffffff;
font-size:16px;
font-weight:bolder;
}
.allowedQuote{
font-size:18px;
text-align:center;
font-weight:bolder;
}
.afterAllowed p{
text-align:center;	
color:#696969;
font-weight: bold;
font-size: 12px;
}
.afterAllowed span{
color:#ff0000;
padding: 0 4px;
}
.afterAllowedBtn{
width:100%;
display:flex;
flex-direction:row;
color:#ffffff;
}
.afterAllowedBtn .msgEngnr{
width:50%;
height: 40px;
padding: 0px;
margin: 5px;
background-color: #33B8FF;
border:none;
}
.afterAllowedBtn .msgEngnr:hover{
background-color: rgb(0, 167, 255);	
}
.afterAllowedBtn .endQuoteForm{
width:50%;
}
.endQuoteForm button{
width:100%;
height: 40px;
padding: 0px;
margin: 5px;
background-color: rgba(160, 166, 170, 0.83);
border:none;
}
.endQuoteForm button:hover{
background-color: rgba(130, 134, 137, 0.83);
}