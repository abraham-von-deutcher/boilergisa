body{
background-color:#cccccc;
}
select{
text-align: center;
vertical-align: middle;
text-align-last: center;
}
.orderHeader{
width:100%;
position: fixed;
top:56px;
line-height: 45px;
background: #f8f8f8;
border:1px solid #efefef;
margin: 0;
padding: 0;
z-index:99;
}
.orderStatus{
width: 100%;
display: flex;
}
.orderStatus li{
width: 20%;
font-size: 11px;
border: 0.5px solid #efefef;
text-align: center;
cursor: pointer;
font-weight: normal;
z-index: 999;
}
.orderStatus li:hover, .orderStatus .on{
color: #FF9900;
background-color: #efefef;
}
.kindsInstall{
width: 100%;
display: flex;
padding-left: 10px;
margin: 0;
background-color: #ffffff;
}

.kindsInstall p{
padding: 0 10px;
margin: 0;
font-size: 14px;
align-self: center;
}
.orderContent{
display: none;
height:100%;
padding-top:152px;
padding-bottom:63px;
}
.orderContent.on{
display:inherit;
}
.orderList li{
height: 145px;
border: 1px solid #d1d1d1;
background: #ffff;
margin: 10px;
display: flex;
cursor:pointer;
}
.orderList li:hover{
background: #efefef;
}
.orderList li img{
width: 38%;
border:1px soild #000000;
margin: 8px 0;
margin-left:8px;
}
.etcList p{
font-size: 13px;
text-align: center;
padding:10px;
}
.infoQuote{
width: 62%;
margin: 8px;
}
.infoInstall .equipment{
color: #ff6c00;
font-size: 13px;
}
.infoInstall .waterheater{
display: inline-block;
color: rgb(149, 157, 201);
font-size: 13px;
}
.dataTime{
float: right;
}
.infoInstall p{
display: inline-block;
font-size: 12px;
margin-top: 1px;
margin-bottom: 1px;
padding-left: 4px;
}
.numbReserv{
color: #33B8FF;
}
.infoAddrStatus{
width:100%;
height:64px;
padding-top:4px;
display: flex;
flex-direction:column;
}
.address{
width: 100%;
height:42px;
font-size: 11px;
padding: 0px;
text-align: left;
}
.sendStatus{
width: 100%;
height:22px;
color: #FF9900;
font-size: 13px;
font-weight:bolder;
align-self: center;
text-align: right;
}
.newStatus{
width: 100%;
height:22px;
color: #33B8FF;
font-size: 12px;
align-self: center;
text-align: right;
}
.finishStatus{
width: 100%;
height:22px;
color: #ff0000;
font-size: 12px;
align-self: center;
text-align: right;
}
.installedStatus{
width: 100%;
height:22px;
color: #909090;
font-size: 12px;
align-self: center;
text-align: right;
}

