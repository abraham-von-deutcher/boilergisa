body{
background-color:#cccccc;
}
ul li:hover{
background: #ffffff;
}
.orderContent{
height:100%;
padding-top: 56px;
padding-bottom:63px;
font-size: 12px;
}
.orderList li{
height: 336px;
border: 1px solid #d1d1d1;
background: #ffffff;
margin: 10px;
padding:8px;
flex-direction: column;
}
.engnrInfo{
width: 100%;
height: 35px;
display: flex;
font-size: 13px;
border-bottom: 0.5px solid #909090;
}
.engnrInfo img{
width: 15%;
margin: 4px;
}
.engnrInfo .engnrName{
width: 55%;
text-align: center;
align-self: center;
}
.engnrInfo .recordDate{
width: 30%;
align-self: center;
text-align: right;
padding: 0 8px;
}

.recordInfo:hover{
background: #efefef;
cursor: pointer;
}
.installInfo{
width: 100%;
text-align: center;
}
.installInfo .installEqu{
font-size: 14px;
font-weight: bold;
padding: 4px;
margin: 0;
}
.installInfo .installAddr{
padding: 4px;
margin: 0;
}
.etcList p{
font-size: 13px;
text-align: center;
padding:10px;
}
.recordImg{
width: 100%;
height: 196px;
}
.recordImg img{
width: 100%;
height: 100%;
}
.recordCommentary{
width:100%;
height:36px;
padding-top: 8px;
}
.recordHit{
float:right;
padding: 5.5px;
}
.addRecord{
width: 60px;
height: 40px;;
color: #ffffff;
border: none;
border-radius: 25%;
font-size: 13px;
padding: 10px;
right: 30px;
position:fixed;
bottom:90px;
background:#FF9900;
}
.recordCommentary .totalRating{
color:#FF0000;
font-weight:bold;
}