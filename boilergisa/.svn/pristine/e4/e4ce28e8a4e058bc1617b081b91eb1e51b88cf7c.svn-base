.main p{
font-size:12px;
}
label{
font-size:11px;
}
.engnrProfil{
width:100%;
height:240px;
outline:none;
}

.inputEngnrInfo{
padding:0 16px;
}
.engnrName p{
font-size:14px;
padding: 6px;
padding-bottom: 0;
font-weight:bolder;
}

.introEngnr .engnrIntro{
text-overflow: ellipsis;
overflow: hidden;
display: -webkit-box;
-webkit-line-clamp: 7;
-webkit-box-orient: vertical;
}
.btn_more{
display:none;
float:right;
padding-right:8px;
color:#33B8FF;
cursor: pointer;	
}
.form-group{
padding:0;
margin:0;
display:flex;
}
.form-group p{
padding:0 16px;
}
.businessTime{
display:flex;
}
.dateInfo{
padding-left: 16px;
}
.timeInfo{
padding-left: 16px;
}
/*----------------REVIEW----------------*/
.gradeInfo{
width:100%;
height:80px;
font-weight: bolder;
border-top: 1px solid #d3d3d3;
text-align:center;
}
.gradeInfo .gradeRecord{
font-size: 18px;
color: #959595;
padding:22px 0 35px 0;
margin:0 auto;
}
.gradeInfo span{
font-size: 25px;
color: #33B8FF;
padding-right: 8px;
}
.engnrRecord{
padding-left: 48px;
}
/*----------------ContactButton----------------*/
.contactMethod{
display: flex;
padding: 8px 0px 24px 0px;
color:#ffffff;
}
.contactMethod .tellEngnr{
width: 50%;
height: 40px;
padding: 0px;
margin: 5px;
border: none;
outline: none;
background-color: rgb(51, 184, 255);
}
.contactMethod .tellEngnr:hover{
background-color: rgb(0, 167, 255);	
}
.contactMethod .msgEngnr{
width: 50%;
height: 40px;
padding: 0px;
margin: 5px;
border: none;
outline: none;
background-color: rgba(160, 166, 170, 0.83);
}
.contactMethod .msgEngnr:hover{
background-color: rgba(130, 134, 137, 0.83);
}
/*----------------EngnrRecordList----------------*/
.orderList{
width: 100%;
background-color: #cccccc;
display: flex;
flex-wrap: wrap;
border-top: 1px solid #d3d3d3;
}
.orderList li{
width: 50%;
padding: 0; 
background-color: #ffffff;
cursor: pointer;
font-size: 10px;
padding:4px;
}
.orderList li:nth-of-type(n+5){
display:none;
}
.orderList li:hover{
background: #ebebeb;
}
.engnrInfo{
width: 100%;
height: 35px;
display: flex;
font-size: 8px;
border-bottom: 0.5px solid #909090;
}
.engnrInfo img{
width: 15%;
margin: 4px;
}
.engnrInfo .engnrName{
width: 55%;
text-align: center;
align-self: center;
white-space: nowrap;
overflow: hidden;
text-overflow: ellipsis;
display:inline-block;
}
.engnrInfo .recordDate{
width: 30%;
align-self: center;
text-align: right;
padding: 0 8px;
}
.engnrInfo:hover{
background: #efefef;
cursor: pointer;
}
.recordInfo:hover{
background: #efefef;
cursor: pointer;
}
.installInfo{
width: 100%;
text-align: center;
}
.installInfo .installEqu{
font-size: 10px;
font-weight: bold;
padding: 4px;
margin: 0;
}
.installInfo .installAddr{
width:100%;
white-space: nowrap;
overflow: hidden;
text-overflow: ellipsis;
display:inline-block;
padding: 4px;
margin: 0;
}
.recordImg{
width: 100%;
height: 100px;
}
.recordImg img{
width: 100%;
height: 100%;
}
.recordCommentary{
width:100%;
height:25px;
padding-top: 4px;
}
.recordHit{
float:right;
padding: 5.5px;
}
.recordCommentary .totalRating{
color:#FF0000;
font-weight:bold;
}
.case-more{
float:right;
font-size:11px;
color:#0000ff;
padding:8px;
}
.commentNul{
width:100%;
height:100px;
background:#f1f1f1;
}
.commentNul p{
text-align:center;
font-size:14px;
margin: auto;
color: #8d8d8d;
padding-top:20px;
}