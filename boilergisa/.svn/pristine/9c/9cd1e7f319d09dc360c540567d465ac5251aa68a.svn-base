.main{
padding-left: 16px;
padding-right: 16px;
padding-top: 80px;
}
p{
font-size: 12px;
}
ul li:hover{
background: none;
}
.quote_card{
border: 1px solid #c0c0c4;
padding: 8px 16px 16px 16px ;
margin-bottom: 10px;
}
.reservNr{
color: #33B8FF;
float:left;
font-size:1em;
padding-left:4px;
font-weight: bold;
}
.myModify{
float:right;
font-size:1em;
padding-right:4px;
font-weight:bold;
}
.info_quote{
width: 100%;
padding: 0;
margin: 0;
display: flex;
flex-direction:row;
}
.info_quote img{
width: 30%;
height: 70px;
margin: 10px;
}
.infoMyreq{
width: 70%;
margin: 16px 10px;
display:flex;
flex-direction:column;
}
.equipmentAndDate{
width:100%;
}
.infoMyreq h1{
font-size: 15px;
color: #ff6c00;
padding:0;
margin:0;
display: inline;
}
.infoSpace{
width:100%;
padding-top: 6px;
font-size:12px;
}
.uploadDate{
float:right;
}
.quote_open{
width: 100%;
border: 1px solid #c0c0c4;
text-align: center;
color: #8d8d8d;
cursor: pointer;
background: none;
font-size:1em;
padding:4px;
}
.quote_open:hover{
background: #efefef;
}
.engnrInfo .engnrName{
width: 55%;
text-align: center;
align-self: center;
font-size: 11px;
white-space: nowrap;
overflow: hidden;
text-overflow: ellipsis;
}
